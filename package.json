{
	"name": "iberia-backend-template",
	"version": "0.1.0",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"prepare": "husky install && run-script-os",
		"prepare:linux": "chmod ug+x .husky/*",
		"prepare:macos": "chmod ug+x .husky/*",
		"prepare:windows": "",
		"release:initialize": "commit-and-tag-version --first-release",
		"release:dry": "commit-and-tag-version --dry-run",
		"release": "commit-and-tag-version"
	},
	"dependencies": {
		"@apollo/subgraph": "2.2.2",
		"@nestjs/apollo": "10.1.7",
		"@nestjs/common": "9.2.1",
		"@nestjs/config": "2.2.0",
		"@nestjs/core": "9.2.1",
		"@nestjs/cqrs": "9.0.1",
		"@nestjs/graphql": "10.1.7",
		"@nestjs/platform-express": "9.2.1",
		"@nestjs/swagger": "6.1.4",
		"apollo-server-core": "3.11.1",
		"apollo-server-express": "3.11.1",
		"graphql": "16.6.0",
		"joi": "17.7.0",
		"reflect-metadata": "0.1.13",
		"rimraf": "3.0.2",
		"rxjs": "7.8.0",
		"uuid-random": "1.3.2"
	},
	"devDependencies": {
		"@commitlint/cli": "17.3.0",
		"@commitlint/config-conventional": "17.3.0",
		"@faker-js/faker": "7.6.0",
		"@nestjs/cli": "9.1.5",
		"@nestjs/schematics": "9.0.3",
		"@nestjs/testing": "9.2.1",
		"@types/express": "4.17.15",
		"@types/jest": "28.1.8",
		"@types/node": "16.0.0",
		"@types/supertest": "2.0.12",
		"@typescript-eslint/eslint-plugin": "5.47.0",
		"commit-and-tag-version": "10.1.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "8.30.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-standard-with-typescript": "24.0.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-n": "15.6.0",
		"eslint-plugin-promise": "6.1.1",
		"husky": "8.0.2",
		"jest": "28.1.3",
		"lint-staged": "13.1.0",
		"prettier": "2.8.1",
		"run-script-os": "1.1.6",
		"source-map-support": "0.5.20",
		"supertest": "6.3.3",
		"ts-jest": "28.0.8",
		"ts-loader": "9.4.2",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.1.1",
		"typescript": "4.9.4",
		"webpack": "5.75.0"
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{md,html,json}": "prettier --write"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"commit-and-tag-version": {
		"skip": {
			"commit": true
		}
	}
}
