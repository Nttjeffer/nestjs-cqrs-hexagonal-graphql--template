{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "src",
	"compilerOptions": {
		"plugins": [
			{
				"name": "@nestjs/graphql",
				"options": {
					"typeFileNameSuffix": [".gqltype.ts", ".gqlinput.ts", ".resolvers.ts"]
				}
			}
		]
	}
}
